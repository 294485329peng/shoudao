  <select id="queryPage" resultType="map" parameterType="map" >
    SELECT
    <include refid="Base_Column_List" />
    FROM WX_AUTO_REPLY
    WHERE 1=1
    <if test="t.ROLE_NAME != null" >
      AND ROLE_NAME LIKE '%${t.ROLE_NAME}%'
    </if>
    ORDER BY CREATE_TIME DESC
  </select>
  <delete id="delete" parameterType="string" >
