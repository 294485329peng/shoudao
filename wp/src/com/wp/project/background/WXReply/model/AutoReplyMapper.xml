    AUTO_REPLY_ID, PUBLISH, TYPE, SAYTEXT, PIC_MEDIA_ID, PIC_ID, PIC_URL, CREATE_TIME,IT_NAME,IT_GROUP_ID,IT_CREATE_TIME,IT_PIC_URL
  </sql>
  <select id="get" resultType="map" parameterType="string" >
    SELECT
    <include refid="Base_Column_List" />
    FROM WX_AUTO_REPLY
    WHERE GZH_ID = #{GZH_ID}
    AND PUBLISH = '1'
  </select>
  <select id="query" resultType="map" parameterType="map" >
    SELECT
    <include refid="Base_Column_List" />
    FROM AC_ROLE
    WHERE 1=1
